{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "baseUrl": "./"
  },

  "include": ["module/**/*.ts", "plugins/**/*.ts", "types/**/*.d.ts", "util/**/*.ts","server.ts",
    "app.ts", "main.ts"]
}
